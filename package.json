{
  "name": "tango-for-dummies",
  "version": "2.0.0",
  "description": "",
  "main": "",
  "keywords": [],
  "author": "Chi",
  "license": "ISC",
  "dependencies": {
    "bootstrap-sass": "^3.4.1",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "node-sass": "^6.0.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-hover": "^1.4.0",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "6.3",
    "react-scripts": "^4.0.3",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.5",
    "cra-bundle-analyzer": "^0.1.1",
    "eslint-config-airbnb": "19.0.4",
    "react-error-overlay": "6.0.9"
  },
  "scripts": {
    "clean": "echo 'CLEANING /app/:'; rm -rf node_modules/ dist/ build/",
    "start": "PORT=3002 env-cmd -f .env react-app-rewired start",
    "build": "export REACT_APP_COMMIT_SHA=$(git rev-parse --short HEAD) && react-app-rewired build",
    "test_watch_fast": "env-cmd -f .env.test jest --watch",
    "test": "set -o pipefail && env-cmd -f .env.test jest --detectOpenHandles --coverage --colors --watchAll=false | tee ./jest_test_coverage.txt",
    "test_watch": "env-cmd -f .env.test jest --watchAll --coverage --detectOpenHandles",
    "eject": "react-app-rewired eject",
    "predeploy": "npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:qa": "env-cmd -f .env.qa npm run build",
    "deploy:qa": "npm run build:qa && wrangler publish -e qa",
    "deploy:staging": "npm run build:staging && wrangler publish -e staging",
    "deploy:production": "npm run build:production && wrangler publish -e production",
    "pipe_coverage": "env-cmd -f .env.test jest --coverage | grep files"
  },
  "engines": {
    "node": "16.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
